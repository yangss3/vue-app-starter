{
  "name": "vue-app-starter",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.ts --fix",
    "lint:style": "stylelint src/**/*.{vue,css} --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unocss/reset": "^0.45.8",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.7",
    "@types/node": "^18.7.8",
    "@types/postcss-preset-env": "^7.7.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@yangss/eslint-config-vue": "^0.2.11",
    "@yangss/stylelint-config-vue": "^0.0.3",
    "eslint": "^8.22.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-preset-env": "^7.8.0",
    "stylelint": "^14.10.0",
    "typescript": "^4.7.4",
    "unocss": "^0.45.8",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.1"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": ["postcss"]
    }
  },
  "lint-staged": {
    "*.(ts|vue)": "eslint --fix",
    "*.(css|vue)": "stylelint --fix"
  }
}
