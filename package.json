{
  "name": "vue-app-starter",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint --ext .vue,.ts . --fix",
    "lint:style": "stylelint src/**/*.{vue,css} --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unocss/reset": "^0.35.2",
    "vue": "^3.2.36"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.5",
    "@types/node": "^17.0.35",
    "@types/postcss-preset-env": "^6.7.3",
    "@vitejs/plugin-vue": "^2.3.3",
    "@yangss/eslint-config-vue": "^0.2.11",
    "@yangss/stylelint-config-vue": "^0.0.3",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "postcss-preset-env": "^7.6.0",
    "stylelint": "^14.8.3",
    "typescript": "^4.7.2",
    "unocss": "^0.35.2",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-vue-components": "^0.19.6",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.16"
  },
  "lint-staged": {
    "*.(ts|vue)": "eslint --fix",
    "*.(css|vue)": "stylelint --fix"
  }
}
