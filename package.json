{
  "name": "vue-app-starter",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unocss/reset": "^0.58.0",
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.4.5",
    "@iconify-json/carbon": "^1.1.25",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.10.4",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.55.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss-preset-env": "^9.3.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.7",
    "vue-tsc": "^1.8.25"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss"
      ]
    }
  },
  "lint-staged": {
    "*.(ts|vue)": "eslint --fix"
  }
}
