{
  "name": "vue-app-starter",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.ts --fix",
    "lint:style": "stylelint src/**/*.{vue,css} --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unocss/reset": "^0.49.4",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.10",
    "@types/node": "^18.7.8",
    "@vitejs/plugin-vue": "^4.0.0",
    "@yangss/eslint-config-vue": "^0.2.14",
    "@yangss/stylelint-config-vue": "^0.0.3",
    "eslint": "^8.34.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.1",
    "postcss-preset-env": "^8.0.1",
    "stylelint": "^14.15.0",
    "typescript": "^4.9.3",
    "unocss": "^0.49.4",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.1",
    "vue-tsc": "^1.0.24"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": ["postcss"]
    }
  },
  "lint-staged": {
    "*.(ts|vue)": "eslint --fix",
    "*.(css|vue)": "stylelint --fix"
  }
}
