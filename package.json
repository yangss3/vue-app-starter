{
  "name": "vue-app-starter",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.ts --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepack": "npx simple-git-hooks"
  },
  "dependencies": {
    "@unocss/reset": "^0.55.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@iconify-json/carbon": "^1.1.19",
    "@types/node": "^18.17.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@yangss/eslint-config-vue": "^0.2.14",
    "eslint": "^8.47.0",
    "lint-staged": "^13.2.3",
    "postcss-preset-env": "^9.1.1",
    "typescript": "^5.1.6",
    "unocss": "^0.55.0",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.1",
    "unplugin-vue-router": "^0.6.4",
    "vite": "^4.4.9",
    "vue-tsc": "^1.8.8"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss"
      ]
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.(ts|vue)": "eslint --fix"
  }
}
