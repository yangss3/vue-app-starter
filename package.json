{
  "name": "vue-app-starter",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@unocss/reset": "^0.59.4",
    "vue": "^3.4.25"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.16.0",
    "@iconify-json/carbon": "^1.1.31",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "postcss-preset-env": "^9.5.9",
    "typescript": "^5.4.5",
    "unocss": "^0.59.4",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.2.10",
    "vue-tsc": "^2.0.14"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "postcss"
      ]
    }
  },
  "lint-staged": {
    "*.(ts|vue)": "eslint --fix"
  }
}
